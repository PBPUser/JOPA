<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450" Foreground="{DynamicResource BasicForeground}"
			 xmlns:controls="clr-namespace:JVOS.Controls;assembly=JVOS.Controls"
			 FontSize="12"
             x:Class="JVOS.EmbededWindows.EaseOfAccess">
	<ScrollViewer>
		<DockPanel Background="{DynamicResource BasicBackground}" x:Name="rootStack">
			<TextBlock DockPanel.Dock="Top" Text="Ease of Access"  FontSize="16" FontWeight="Bold" Margin="8"/>
			
			
		</DockPanel>
	</ScrollViewer>
</UserControl>
