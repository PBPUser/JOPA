<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
			 xmlns:controls="clr-namespace:JVOS.Controls;assembly=JVOS.Controls"
             x:Class="JVOS.EmbededWindows.Preferences.DisplayPage">
	<StackPanel x:Name="rootStack">
		<ComboBox />
		<StackPanel Orientation="Horizontal" HorizontalAlignment="Center" Margin="8">
				<controls:JButton FontSize="14" x:Name="apply" Content="Apply" Width="100"/>
		</StackPanel>
	</StackPanel>
</UserControl>
