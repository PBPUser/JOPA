<api:HubWindow xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             xmlns:controls="clr-namespace:JVOS.Controls;assembly=JVOS.Controls"
             x:Class="JVOS.Hubs.ClockHub" ClipToBounds="false"
			 xmlns:local="clr-namespace:JVOS.Hubs"
			xmlns:api="clr-namespace:JVOS.ApplicationAPI.Hub;assembly=JVOS.ApplicationAPI">
	<Grid Margin="8">
		<controls:CustomBlurBehind CornerRadius="12"/>
		<Border Classes="Bar Alpha Outer">
			<StackPanel>
				<TextBlock Text="1488" FontSize="64" FontWeight="Bold" Foreground="{DynamicResource BarForeground}" Margin="48, 32"/>
				<Border Margin="8" Classes="Transparent Alpha Bar Inner">
					<Grid>
						<Grid.ColumnDefinitions>
							<ColumnDefinition/>
							<ColumnDefinition/>
						</Grid.ColumnDefinitions>
						<controls:JButton Classes="Bar Alpha Transparent" Height="32" x:Name="_setLeft" Content="Left"/>
						<controls:JButton Classes="Bar Alpha Transparent" Height="32" x:Name="_setCenter" Content="Right" Grid.Column="1"/>
					</Grid>
				</Border>
			</StackPanel>
		</Border>
	</Grid>
</api:HubWindow>
