<api:HubWindow xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             xmlns:controls="clr-namespace:JVOS.Controls;assembly=JVOS.Controls"
             x:Class="JVOS.Hubs.VolumeHub" ClipToBounds="false"
			 xmlns:local="clr-namespace:JVOS.Hubs"
			xmlns:api="clr-namespace:JVOS.ApplicationAPI.Hub;assembly=JVOS.ApplicationAPI">
	<Grid Margin="8">
		<controls:CustomBlurBehind CornerRadius="12"/>
		<Border Classes="Bar Alpha Outer" MinHeight="40" MinWidth="256">
			<StackPanel>
				<TextBlock Text="Volume" DockPanel.Dock="Top" Classes="h1 Bar"/>
				<Slider x:Name="sliderVolume" Minimum="0" Maximum="100"/>
			</StackPanel>
		</Border>
	</Grid>
</api:HubWindow>
